<robot name="sensapex_manipulator">

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.063 0.048 0.025"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.063 0.048 0.025"/>
      </geometry>
    </collision>
  </link>

  <link name="y_axis_link">
    <visual>
      <geometry>
        <box size="0.05 0.02 0.055"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.02 0.055"/>
      </geometry>
    </collision>
  </link>

  <joint name="y_axis_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="y_axis_link"/>
    <origin xyz="0 0.02 0.04" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.01" upper="0.01" effort="1000" velocity="0.5"/>
  </joint>

  <link name="z_axis_link">
    <visual>
      <geometry>
        <box size="0.048 0.015 0.055"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.048 0.015 0.055"/>
      </geometry>
    </collision>
  </link>

  <joint name="z_axis_joint" type="prismatic">
    <parent link="y_axis_link"/>
    <child link="z_axis_link"/>
    <origin xyz="0 0.02 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.01" upper="0.01" effort="1000" velocity="0.5"/>
  </joint>

  <link name="xz_rotation_link">
    <visual>
      <geometry>
        <box size="0.005 0.005 0.005"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>

  <joint name="xz_rotation" type="revolute">
    <parent link="z_axis_link"/>
    <child link="xz_rotation_link"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1000" velocity="0.5"/>
  </joint>

  <link name="x_axis_link">
    <visual>
      <geometry>
        <box size="0.035 0.07 0.04"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.035 0.07 0.04"/>
      </geometry>
    </collision>
  </link>

  <joint name="x_axis_joint" type="prismatic">
    <parent link="xz_rotation_link"/>
    <child link="x_axis_link"/>
    <origin xyz="-0.005 0 0" rpy="-0.3927 0 0"/>  <!-- pi/8 -->
    <axis xyz="0 0.5 0.866"/>  <!-- TODO this should be ~67.5Â° -->
    <limit lower="0" upper="0.02" effort="1000" velocity="0.5"/>
  </joint>

  <link name="end_effector">
    <visual>
      <geometry>
        <cylinder radius="0.01" length="0.01"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.01" length="0.01"/>
      </geometry>
    </collision>
  </link>

  <joint name="end_effector_joint" type="fixed">
    <parent link="x_axis_link"/>
    <child link="end_effector"/>
    <origin xyz="-0.02 0.03 0" rpy="-1.5708 0 0"/>
  </joint>

</robot>
